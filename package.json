{
  "name": "syncer-music",
  "version": "1.0.0",
  "description": "Application de synchronisation de playlists entre Spotify, Deezer et YouTube Music",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": ["music", "playlist", "sync", "spotify", "deezer", "youtube-music"],
  "author": "Syncer Music",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "winston": "^3.10.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "passport": "^0.6.0",
    "passport-spotify": "^2.0.0",
    "passport-oauth2": "^1.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
} 