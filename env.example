# Variables d'environnement pour Syncer Music

# Configuration serveur
PORT=3001
FRONTEND_URL=https://your-vercel-app.vercel.app

# Configuration Spotify
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_REDIRECT_URI=https://your-vercel-app.vercel.app/api/spotify-callback

# Configuration Deezer
DEEZER_APP_ID=your_deezer_app_id
DEEZER_SECRET_KEY=your_deezer_secret_key
DEEZER_REDIRECT_URI=https://your-vercel-app.vercel.app/api/deezer-callback

# Configuration Google/YouTube
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://your-vercel-app.vercel.app/api/google-callback 